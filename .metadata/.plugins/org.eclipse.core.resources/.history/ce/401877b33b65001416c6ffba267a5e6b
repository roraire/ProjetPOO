package Vue;

import java.awt.Graphics;

import javax.swing.JPanel;

public class JeuPanel extends JPanel {
	
	
	public JeuPanel(){
		
		
	}
	
	public void updateDisplay(){
		repaint();
   	}
	public void paintComponent(Graphics g) {
		super.paintComponent(g);
		g.drawLine(30, 250, 150, 100);
	}

}
